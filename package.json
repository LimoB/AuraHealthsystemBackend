{
  "name": "aura-medicare",
  "version": "1.0.0",
  "description": "Backend API for the medical system built with Express, Drizzle ORM, and TypeScript",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json src/server.ts",
    "start": "node ./dist/server.js",
    "build": "tsup src/server.ts --outDir dist --format esm --target es2022 --clean",
    "seed": "tsx src/drizzle/seed.ts",
    "migrate": "drizzle-kit push",
    "gen": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio"
  },
  "keywords": [],
  "author": "Ascah Moraa",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "drizzle-orm": "^0.44.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.4",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^7.1.1",
    "stripe": "^18.4.0",
    "swagger-express-router": "^1.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.7",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "drizzle-kit": "^0.31.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
